<?xml version="1.0" encoding="utf-8" ?>
<BehaviourTree>
	<Sequence>
		<Executor class="RiseFromChair"/>
		<Executor class="MoveToVendingMachine" money="200"/>
		<RandomSelector>
			<Executor class="BuyTea"/>
			<Executor class="BuyCoffee"/>
		</RandomSelector>
		<Selector>
			<Inverter>
				<Executor class="BuyTea"/>
			</Inverter>
			<Executor class="BuyCoffee"/>
		</Selector>
		<Executor class="ReturnToChair"/>
		<Repeater repeat_count="10">
			<Executor class="BuyCoffee"/>
		</Repeater>
	</Sequence>
</BehaviourTree>