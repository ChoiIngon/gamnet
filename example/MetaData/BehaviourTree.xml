<?xml version="1.0" encoding="utf-8" ?>
<BehaviourTree>
	<Sequence>
		<Action class="SearchTarget"/>
		<Action class="MoveToTarget"/>
		<RandomSelector>
			<Action class="BuyTea"/>
			<Action class="BuyCoffee"/>
		</RandomSelector>
		<Selector>
			<Inverter>
				<Action class="BuyTea"/>
			</Inverter>
			<Action class="BuyCoffee"/>
		</Selector>
		<Action class="ReturnToChair"/>
		<Repeater repeat_count="10">
			<Action class="BuyCoffee"/>
		</Repeater>
	</Sequence>
</BehaviourTree>